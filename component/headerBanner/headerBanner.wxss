.banner{
    box-sizing: border-box;
    position: relative;
    height: 340rpx;
    background-image: url("http://qiniucdn.dailyyoga.com.cn/c9/de/c9de408ecebbd105ba77287e577bcf7e.jpeg");
    background-size: cover;
}
.withdraw button{
    float: right;
    margin-top: 24rpx;
    margin-right: 20rpx;
    height: 52rpx;
    line-height: 36rpx;
    padding: 8rpx 24rpx;
    font-size: 24rpx;
    border: 1px solid #fff;
    color: #fff;
    border-radius: 52rpx;
    background: none;
    z-index: 3;
}
.money-area{
    position: absolute;
    top: 50%;
    margin-top: -50rpx;
    text-align: center;
    width: 100%;
    color: #fff;
}
.money-area view{
    font-size: 30rpx;
    line-height: 30rpx;
}
.money-area .money{
    font-size: 68rpx;
    color: #fff;
    line-height: 68rpx;
    margin-bottom: 24rpx;
}
.banner-bg {
    position: absolute;
    width: 236rpx;
    height: 200rpx;
    left: 0;
    bottom: 0;
}
.over-tip{
    display: block;
    text-align: center;
    font-size: 24rpx;
    line-height: 24rpx;
    color: #fff;
    margin-top: 24rpx;
}