video.session{
    width: 100%;
    height: 422rpx;
}
.session-desc{
    justify-content: space-between;
    background: #fff;
    height: 100rpx;
    padding: 0 30rpx 0 30rpx;
}

.gif-area{
    height: 40rpx;
    line-height: 40rpx;
    font-size: 28rpx;
    color: #333;
    background: url("data:image/png;base64,") no-repeat left center;
    background-size: 32rpx 32rpx;
    padding-left: 38rpx;
}

.gif-area::after{
    border: none;
}

.session-tip{
    margin-top: 20rpx;
    background: #fff;
    padding: 0 20rpx;
    margin-bottom: 100rpx;
}
.session-tip .title{
    height: 90rpx;
    line-height: 90rpx;
    color: #333;
    font-size: 28rpx;
}
.session-tip image{
    width: 700rpx;
    height: 700rpx;
}
.btn-area{
    position: fixed;
    width: 100%;
    bottom: 0;
    z-index: 2;
}
.btn-area button{
    width: 100%;
    border-radius: 0;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 32rpx;
}
.share-area{
    position: absolute;
    width: 100%;
    bottom: 0;
    background: #fff;
}
.share-area button.cancel{
    margin-top: 20rpx;
    background: #fff;
    font-size: 28rpx;
    color: #666;
    height: 90rpx;
    line-height: 90rpx;
}

.bottom-share{
    position: fixed;
    z-index: 5;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    top: 0;
}

.dialog-area{
    z-index: 10;
    width: 540rpx;
    height: 464rpx;
    box-sizing: border-box;
    border-radius: 16rpx;
    padding: 114rpx 10rpx 24rpx 10rpx;
    background-color: #fff;
    margin: 0 auto;
    margin-top: 320rpx;
    background-image: url("http://qiniucdn.dailyyoga.com.cn/d8/07/d80732a188af73510d59735777dc2513.png");
    background-size: 100% auto;
    background-repeat: no-repeat;
}
.dialog-area .cloud{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: auto;
    z-index: -10;
}
.dialog-area .session-name{
    margin-top: 16rpx;
}
.dialog-area .practice{
    margin-top: 38rpx;
}
.dialog-area .practice .main{
    font-size: 56rpx;
    color: #666666;
    padding-bottom: 4rpx;
}
.bottom-share .header{
    position: absolute;
    z-index: 20;
    width: 360rpx;
    height: 360rpx;
    left: 50%;
    margin-left: -180rpx;
    top: 150rpx;
}
.mark-btn{
    width: 324rpx;
    height: 72rpx;
    line-height: 72rpx;
    margin: 0 auto;
    margin-top: 24rpx;
    font-size: 32rpx;
    color: #fff;
    border-radius: 3px;
    background: #8CA5FF;
}

