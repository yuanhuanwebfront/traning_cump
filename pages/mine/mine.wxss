.banner{
    position: relative;
    height: 280rpx;
    justify-content: space-between;
}
.banner view.bg{
    position: absolute;
    top: 0;
    left: 0; 
    width: 100%;
    height: 280rpx;
}
.banner view.bg:last-child{
  z-index: 1;
  background-image: -webkit-linear-gradient(left top, rgba(97, 132, 234, 0), #996FF2)
}
.banner view.bg:first-child{
  z-index: 0;
  background-image: -webkit-radial-gradient(left top, rgba(1, 213, 227, 1), #6184EA)
}
.ab{
    position: absolute;
    top: 50%;
}
view.ab{
    flex-wrap: wrap;
    justify-content: flex-start;
    left: 280rpx;
    height: 100rpx;
    margin-top: -50rpx;
}
.banner image{
    left: 108rpx;
    margin-top: -62.5rpx;
    width: 125rpx;
    height: 125rpx;
    border: 4rpx solid #fff;
    border-radius: 100rpx;
    box-sizing: border-box;
    z-index: 2;
}
.banner text{
    top: 100rpx;
    display: block;
    width: 100%;
    height: 48rpx;
    line-height: 48rpx;
    color: #fff;
}

view.name{
    height: 70rpx;
    line-height: 70rpx;
    background: #fff;
}
.bar{
    height: 100rpx;
    padding-left: 110rpx;
    line-height: 100rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-color: #fff;
    background-size: 48rpx 420rpx; 
    background-position-x: 40rpx;
    border-bottom: 1px solid #fafafa;
}
.bar:first-child{
    background-position-y: 0rpx;
}
.bar.daily{
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 48rpx 48rpx;
    background-position: 40rpx center;
}