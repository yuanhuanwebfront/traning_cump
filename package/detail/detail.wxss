.detail-title {
    padding: 24rpx 24rpx 24rpx 29rpx;
    background: #fff;
    box-sizing: border-box;
    margin-bottom: 20rpx;
    overflow: hidden;
}
.detail-banner image{
    display: block;
}
.detail-title .flex {
    margin-top: 16rpx;
    justify-content: space-between;
}
.detail-content{
    padding-bottom: 100rpx;
}

.tab {
    height: 90rpx;
    background: #fff;
}
.tab view.word-16{
    box-sizing: border-box;
    min-width: 120rpx;
    line-height: 90rpx;
    height: 90rpx;
}
.tab .border{
    font-size: 22rpx;
    color: #8CA5FF;
    padding: 0 6rpx;
    margin-left: 6rpx;
    height: 30rpx;
    line-height: 30rpx;
}
.tab .word-16.active{border-bottom: 4rpx solid #333;}

.detail-content .vip-border{
    margin-top: 20rpx;
    margin-bottom: 20rpx;
    width: 660rpx;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 72rpx;
}
.session-title{
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 104rpx;
    margin-top: 20rpx;
    color: #999;
    background-color: #fff;
    border-bottom: 1px solid #f1f1f1;
}
.session-title:first-child{
    margin-top: 0;
}
.session-intro{
    height: 150rpx;
    background: #fff;
    justify-content: space-between;
    padding: 0 48rpx;
    border-bottom: 1px solid #f1f1f1;
    margin-bottom: 20rpx;
}
.session-intro .detail{
    padding-left: 90rpx;
    background: url("http://qiniucdn.dailyyoga.com.cn/95/09/95090b5253069babd820239d39f76e4a.png") no-repeat left center;
    background-size: 36rpx 52rpx;
}
.session-intro .detail.active{
    background-image: url("http://qiniucdn.dailyyoga.com.cn/87/a8/87a85255dc9caf33e0dc0343901dff19.png");
}

.session-intro .detail view{margin-bottom: 8rpx;}
.session-intro .vip-color{
    display: inline-block;
    border-radius: 60rpx;
    padding: 4rpx 16rpx;
}
.icon-kefu{
    margin-top: 14rpx;
    font-size: 40rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-bottom: 4rpx;
}

.bottom-area {
    position: fixed;
    background: #fff;
    bottom: 0;
    width: 100%;
    height: 100rpx;
}

.bottom-area button.service {
    width: 140rpx;
    height: 100rpx;
    line-height: 24rpx;
    background-color: #fff;
    background-image: url("data:image/png;base64,");
    background-size: cover;
}

.bottom-area button.service.share{
    position: relative;
    overflow: auto;
    background-image: url("data:image/png;base64,");
}

.bottom-area button.btn-confirm{
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    border-radius: 0;
    font-size: 36rpx;
}

.back-home{
    position: fixed;
    padding: 0;
    bottom: 120rpx;
    right: 20rpx;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    background-color: #C59D53;
    font-size: 20rpx;
    color: #fff;
    border-radius: 80rpx;
}

.bottom-share{
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
    top: 0;
    z-index: 10;
}
.share-area{
    position: absolute;
    width: 100%;
    bottom: 0;
    background: #fff;
    /*padding-top: 60rpx;*/
}
.share-area button.cancel{
    margin-top: 20rpx;
    background: #fff;
    font-size: 28rpx;
    color: #666;
    height: 90rpx;
    line-height: 90rpx;
}

.rebate-rule{
    margin: 20rpx 0 40rpx 0;
    font-size: 24rpx;
    padding: 0 20rpx;
    text-align: right;
}

.rebate-rule text{
    background-image: url("data:image/png;base64,");
    background-size: 28rpx 28rpx;
    background-repeat: no-repeat;
    background-position: left center;
    padding-left: 36rpx;
}

.share-area .center.time{
    font-size: 22rpx;
    background: #FA862A;
    border-radius: 3px;
    width: fit-content;
    margin: 0 auto;
    padding: 0 16rpx;
    line-height: 32rpx;
    height: 32rpx;
    color: #fff;
    margin-bottom: 30rpx;
}

text.fix-word{
    content: "分享返现";
    position: fixed;
    left: 130rpx;
    bottom: 90rpx;
    z-index: 5;
    display: block;
    background: #C59D53;
    padding: 0 16rpx;
    height: 32rpx;
    line-height: 32rpx;
    text-align: center;
    font-size: 20rpx;
    color: #fff;
    border-radius: 20rpx;
}

text.fix-tran{
    content: "";
    position: fixed;
    z-index: 5;
    bottom: 80rpx;
    left: 150rpx;
    width: 0;
    height: 0;
    border-left:12rpx solid transparent;
    border-right:12rpx solid transparent;
    border-top:12rpx solid #C59D53;
}