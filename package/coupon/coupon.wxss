.coupon-box{
    padding: 25rpx 28rpx;
    height: 40rpx;
    line-height: 40rpx;
    color: #666;
}
.coupon-box .iconfont{
    color: #bdbdbd;
    line-height: 40rpx;
    font-size: 30rpx;
    margin-right: 10rpx;
}
.coupon-item{
    position: relative;
    width: 710rpx;
    height: 232rpx;
    margin: 0 auto;
    margin-bottom: 20rpx;
    box-sizing: border-box;
    justify-content: space-between;
    background-image: url("data:image/png;base64,");
    background-size: cover;
}
.coupon-item.disable{background-image: url("data:image/png;base64,");}
.coupon-item .price{
    width: 170rpx;
    margin-left: 26rpx;
    font-size: 40rpx;
    color: #fff;
}
.coupon-item .intro{
    width: 100%;
    height: 230rpx;
    margin-left: 80rpx;
}
.intro .vip-border{
    width: 90%;
    padding: 16rpx 0;
    font-size: 30rpx;
    color: #D9B674;
    border: none;
    border-bottom: 1rpx solid #D9B674;
}
.desc-area{padding-top: 30rpx;}
.intro .desc{
    width: 85%;
    font-size: 22rpx;
    color: #666;
    margin-bottom: 10rpx;
    line-height: 32rpx;
}
.coupon-item.disable .vip-border{
    color: #999;
    border-color: #999;
}
.coupon-item.disable .desc{color: #999;}
.coupon-item .radio{
    position: absolute;
    box-sizing: border-box;
    right: 16rpx;
    border: 1px solid #D9B674;
    width: 40rpx;
    height: 40rpx;
    border-radius: 40rpx;
}
.coupon-item .radio.select-bg{
    border: none;
}
.expire-tag{
    position: absolute;
    top: 0;
    right: 0;
    width: 70rpx;
    height: 70rpx;
    background-image: url("data:image/png;base64,");
    background-size: cover;
}
.used-tag{
    background-image: url("data:image/png;base64,");
}
.empty-img{
    display: block;
    width: 420rpx;
    height: 340rpx;
    margin: 40rpx auto;
}