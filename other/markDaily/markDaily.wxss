.header{
    position: relative;
    width: 100%;
    height: 236rpx;
    justify-content: flex-start;
    padding: 0 32rpx;
    box-sizing: border-box;
    background-image: -webkit-linear-gradient(left top, #14c4e4, #996FF2)
}
.header image{
    width: 240rpx;
    height: 135rpx;
    border: 4rpx solid #fff;
}
.header .arrow{
    position: absolute;
    right: 30rpx;
    bottom: 105rpx;
    border: none;
    width: 18rpx;
    height: 26rpx;
}
.session-desc{margin-left: 24rpx;}
.session-desc .name{
    margin-bottom: 22rpx;
    color: #FFF;
}
.session-desc .periods{
    background: #F5E521;
    border-radius: 6rpx;
    width: fit-content;
    height: 34rpx;
    line-height: 34rpx;
    padding: 0 24rpx;
    font-size: 22rpx;
    color: #51370E;
    margin-bottom: 14rpx;
}
.session-desc .date{color: #FFFFFF;}
.info{
    height: 124rpx;
    background: #fff;
    margin-bottom: 22rpx;
}
.info .center view{margin-top: 6rpx;}
.tab{
    position: relative;
    background: #fff;
    width: 100%;
    height: 100rpx;
    overflow: visible;
    border-bottom: 1px solid rgba(216,216,216,0.50);
}
.tab-item{
    position: absolute;
    bottom: 0;
    z-index: 10;
    width: 240rpx;
    height: 80rpx;
    line-height: 80rpx;
}
.tab-item.class{left: 68rpx;}
.tab-item.mine{right: 68rpx;}
.tab-item.active{
    bottom: -2rpx;
    color: #8CA5FF;
    border-bottom: 4rpx solid #8CA5FF;
}
.daily-content{
    padding: 0 28rpx;
    margin-bottom: 22rpx;
    background: #fff;
}
.person{
    padding-top: 30rpx;
    justify-content: flex-start;
}
.person image{
    width: 88rpx;
    height: 88rpx;
    border-radius: 44rpx;
    margin-right: 20rpx;
}
.person .word-15{margin-bottom: 14rpx;}
.content{margin: 28rpx 0 20rpx 0;}
.info-area image{margin-bottom: 24rpx;}
.info-area .zan{
    display: inline-block;
    height: 40rpx;
    line-height: 40rpx;
    padding-left: 40rpx;
    text-align: right;
    background: url("data:image/png;base64,") no-repeat left center;
    background-size: 34rpx 32rpx;
}
.zan.active{background-image: url("data:image/png;base64,");}
.zan-list{
    display: flex;
    align-items: flex-start;
    flex-wrap: wrap;
    padding: 16rpx 0;
    color: #576B95;
    padding-left: 64rpx;
    margin-top: 16rpx;
    background: url("data:image/png;base64,") no-repeat 20rpx 20rpx;
    background-size: 28rpx 24rpx;
    background-color: #F4F3F5;
    overflow: visible;
}
.zan-list::before{
    content: '';
    position: absolute;
    right: 16rpx;
    top: -10rpx;
    z-index: 10;
    width: 0;
    height: 0;
    border-left: 20rpx solid transparent;
    border-right: 20rpx solid transparent;
    border-bottom: 20rpx solid #F4F3F5;
}
.zan-list text{word-break: break-all;}
.btn-share{
    position: fixed;
    z-index: 20;
    padding: 0;
    right: 20rpx;
    bottom: 120rpx;
    width: 90rpx;
    height: 90rpx;
    line-height: 90rpx;
    color: #fff;
    border-radius: 90rpx;
    background: rgba(0, 0, 0, 0.8);
}
.day-tip{
    box-sizing: border-box;
    height: 60rpx;
    line-height: 60rpx;
    padding-left: 71rpx;
    background-size: 66rpx 22rpx;
    background-position: left 22rpx;
    border-bottom: 1px solid #f1f1f1;
}
view.back-home{
    height: 100rpx;
    box-sizing: border-box;
    display: flex;
    align-items: center;
    padding-left: 140rpx;
    color: #fff;
    background: url("data:image/png;base64,") no-repeat 24rpx center;
    background-size: 88rpx 88rpx;
    background-color: rgba(0, 0, 0, 0.7);
}
.back-home button{
    display: inline-block;
    width: 202rpx;
    height: 60rpx;
    color: #fff;
    line-height: 60rpx;
    margin-right: 30rpx;
    background-image: linear-gradient(90deg, #8899F3 0%, #C0C7FF 99%);
    border-radius: 60rpx;
}
.bottom{
    position: fixed;
    display: flex;
    align-items: center;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 160rpx;
    z-index: 20;
}
view.bottom .arrow{bottom: 67rpx;}
.ranking{position: relative;}
.ranking image{
    position: absolute;
    left: 10rpx;
    width: 48rpx;
    height: 48rpx;
    border-radius: 24rpx;
    z-index: 2;
}
.empty{margin-top: 100rpx;}
.empty image{
    width: 336rpx;
    height: 278rpx;
}
.empty .word{margin-top: 24rpx;}
.empty .btn-confirm{
    width: 260rpx;
    height: 72rpx;
    line-height: 72rpx;
    border-radius: 100rpx;
    font-size: 30rpx;
    margin-top: 40rpx;
}
