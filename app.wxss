/*  iconFont    引入  */
@import './assets/iconfont.wxss';
@import './lib/wxParse/wxParse.wxss';

/*              初始化小程序标签  原始样式                */
page {
    height: 100%;
}
image {
    width: 100%;
}
view, text {
    font-size: 24rpx;
}
::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}
button::after {
    border: none;
}
input {
    height: 60rpx;
    line-height: 60rpx;
    border-bottom: 2rpx solid #979797;
}


/*                  通用样式                    */
.flex {
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.over-hide {
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.center {text-align: center;}
.word-left {text-align: left;}
.relative{position: relative;}
.inline {display: inline-block;}
.left {float: left;}
.right {float: right;}
.del{text-decoration: line-through;}

.container {
    position: relative;
    width: 100%;
    min-height: 100%;
    box-sizing: border-box;
    background: #fafafa;
}

.border {
    border: 1px solid #8CA5FF;
    border-radius: 6rpx;
}

.vip-border {
    border: 1px solid #C59D53;
    font-size: 30rpx;
    color: #C59D53;
}

.vip-color {
    color: #fff;
    background: #C59D53;
}

.item-border {
    border-bottom: 2rpx solid #e5e5e5;
}
view.split-area{
    height: 20rpx;
    background-color: #f9fafa;
}

/*  文字样式*/
.money {color: #FA862A !important;}
.vip {color: #FD7E10 !important;}
.info-color {color: #C59D53 !important;}
.color-3{color: #333;}
.color-6{color: #666;}
.color-9{color: #999;}
.color-c8{color: #c8c8c8;}
.color-price{color: #FE6B42;}
.color-man{color: #EE6723;}
.word-10{font-size: 20rpx;}
.word-11{font-size: 22rpx;}
.word-12{font-size: 24rpx;}
.word-13{font-size: 26rpx;}
.word-14{font-size: 28rpx;}
.word-15{font-size: 30rpx;}
.word-16{font-size: 32rpx;}
.word-17{font-size: 34rpx;}
.word-18{font-size: 36rpx;}
.word-20{font-size: 40rpx;}
.word-32{font-size: 64rpx;}

.active-word {
    background: #8CA5FF;
    color: #fff;
}
.pla-word {
    font-size: 32rpx;
    color: #999;
    padding-left: 8rpx;
}

/*  按钮样式    */
.btn {
    outline: none;
    width: 260rpx;
    height: 72rpx;
    border-radius: 100rpx;
    border: 2rpx solid #8CA5FF;
    color: #8CA5FF;
    font-size: 28rpx;
    padding: 0;
    margin: 0;
}
.btn-confirm {
    background: #8CA5FF;
    color: #fff;
    border: none;
}
/*  背景样式    */
.bg-f{background-color: #fff !important;}
.day-bg {
    background-image: url("data:image/png;base64,");
    background-size: 66rpx 20rpx;
    background-repeat: no-repeat;
    background-position: 34rpx center;
}
.back-bg {
    background: url('data:image/png;base64,') no-repeat right 20rpx center;
    background-size: 16rpx 28rpx;
}
.reward-bg {
    background: url("data:image/png;base64,") no-repeat left center;
    background-size: 38rpx 52rpx;
}
.select-bg {
    background-image: url("data:image/png;base64,");
    background-size: cover;
}
.reward-bg.active {
    background-image: url("data:image/png;base64,");
}
.black-bg{
    position: fixed;
    z-index: 999;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.6);
}


/*  通用的返利文案样式 */
.common-rebate-area{
    margin-bottom: 20rpx;
}

.common-rebate-area .money{
    color: #D9B674;
    font-size: 32rpx;
}

.common-rebate-time{
    margin: 0 auto;
    margin-bottom: 36rpx;
    color: #fff;
    background: #FA862A;
    width: fit-content;
    border-radius: 6rpx;
    height: 32rpx;
    line-height: 32rpx;
    padding: 0 16rpx;
}

.rebate-rule{
    margin: 20rpx 0 40rpx 0;
    font-size: 24rpx;
    padding: 0 20rpx;
    text-align: right;
}

.rebate-rule text{
    background-image: url("data:image/png;base64,");
    background-size: 28rpx 28rpx;
    background-repeat: no-repeat;
    background-position: left center;
    padding-left: 36rpx;
}



