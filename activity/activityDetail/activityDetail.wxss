.banner{
    height: 430rpx;
    display: block;
}
.banner.finish-session::after{
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 430rpx;
    background: url("http://qiniucdn.dailyyoga.com.cn/f9/e2/f9e2cb4b28e22f49dea83efb99d1c042.png") no-repeat center center;
    background-color: rgba(0, 0, 0, 0.4);
    background-size: 380rpx 320rpx;
    content: "";
}
.orange-bg{background-color: #FE6B42;}
.area{
    float: left;
    width: 60%;
    height: 112rpx;
    box-sizing: border-box;
    justify-content: flex-start;
    align-items: flex-end;
}
.area.time-area{width: 40%;}
.finish-session .price-area{
    background-image: linear-gradient(45deg, #B4B4B4 0%, #D7D7D7 100%);
}
.finish-session .count-item{background: #ABABAB;}
.price-area{
    background-image: linear-gradient(45deg, #F095FF 0%, #F64848 100%);
    color: #fff;
}
.price-1{
    padding-left: 32rpx;
    padding-bottom: 4rpx;
}
.price-2{
    padding-left: 50rpx;
    padding-bottom: 12rpx;
}
.time-area{background: #FFEFEA;}
.time-area .center{
    margin-top: 14rpx;
}
.time-ending text{display: inline-block;}
.count-item{
    box-sizing: border-box;
    width: 36rpx;
    text-align: center;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 10rpx;
    color: #fff;
}
.split{padding: 0 8rpx;}
.day-count{margin-right: 10rpx;}
.share-area{
    justify-content: space-between;
    padding: 0 30rpx;
    height: 132rpx;
}
.share-area .word-18{
    margin-bottom: 12rpx;
}
.share-area .share-img{
    padding-left: 36rpx;
    height: 40rpx;
    line-height: 40rpx;
    /*  tip 分享icon */
    background: url("data:image/png;base64,") no-repeat left center;
    background-size: 28rpx 28rpx;
}
.invite-area{
    height: 100rpx;
    padding: 0 28rpx;
    justify-content: space-between;
    border-bottom: 2rpx solid rgba(216,216,216,0.50);
}
.invite-area button{
    width: 140rpx;
    height: 52rpx;
    line-height: 52rpx;
    border-radius: 26rpx;
    color: #fff;
    padding: 0;
    margin: 0;
}
.invite-area button.sign{
    border: 2rpx solid #FE6B42;
    color: #FE6B42;
    background: #fff;
}
.rule{
    height: 70rpx;
    line-height: 70rpx;
    padding: 0 30rpx;
}
.rule .color-6{padding-right: 20rpx;}
.bottom-area{
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    box-sizing: border-box;
    height: 100rpx;
}
.bottom-area view{height: 100rpx;}
.bottom-area .home{
    width: 120rpx;
    border-right: 2rpx solid #f1f1f1;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: cover;
    background-color: #fff;
}
.bottom-area .buy{
  width: 176rpx;
  background-color: #fff;
  box-sizing: border-box;
}
.bottom-area .buy text{
  display: block;
  width: 100%;
  margin: 0;
  padding: 0;
}
.bottom-area .buy .color-price{
    margin-top: 18rpx;
    margin-bottom: 8rpx;
    height: 40rpx;
    line-height: 40rpx;
}
.bottom-area .orange-bg{
    flex-grow: 1;
    color: #fff;
    line-height: 100rpx;
    font-weight: 800;
    border-radius: 0;
}
.user-image{
    width: 70rpx;
    height: 70rpx;
    border-radius: 35rpx;
    margin-right: 20rpx;
}
.share-bg{
    position: fixed;
    width: 100%;
    height: 100%;
    z-index: 20;
    top: 0;
    left: 0;
    background-color: rgba(0, 0, 0, 0.4);
}
.share-content{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    min-height: 716rpx;
    padding-bottom: 20rpx;
    color: #fff;
    background-image: linear-gradient(-45deg, #F64848 0%, #F095FF 100%);
}
.share-content .word-20{
    margin-top: 51rpx;
}
.share-content .main{
    margin: 33rpx auto;
    width: 640rpx;
    background: #fff;
    border-radius: 20rpx;
    padding-bottom: 28rpx;
}
.share-content .time-line{
    display: flex;
    justify-content: center;
    align-items: center;
    height: 48rpx;
    line-height: 48rpx;
    margin-top: 24rpx;
    margin-bottom: 60rpx;
}
.share-content .time-ending{margin-right: 24rpx;}
.invite-person{
    margin-bottom: 30rpx;
}
.invite-person .inline{
    position: relative;
    box-sizing: border-box;
    z-index: 3;
    width: 72rpx;
    height: 72rpx;
    border-radius: 42rpx;
    background-color: #eee;
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-position: center center;
    background-size: 70% 70%;
    margin: 0 15rpx;
    border: 2rpx dashed #cfcfcf;
}
.invite-person .inline.has-img{
    border: none;
}
.invite-person .inline.float-img{
    position: absolute;
    right: 46rpx;
    z-index: 2;
}right
.share-btn-area{
    margin-top: 40rpx;
}
.share-class .share-btn text{
    color: #fff;
}
.end-info{
    position: relative;
    overflow: visible;
    width: 540rpx;
    height: 248rpx;
    margin: 0 auto;
    margin-top: 400rpx;
    border-radius: 20rpx;
    background: #fff;
    background: url("http://qiniucdn.dailyyoga.com.cn/d8/07/d80732a188af73510d59735777dc2513.png") no-repeat left top;
    background-size: 100% auto;
    background-color: #fff;
}
.end-info .word-18{
    padding-top: 150rpx;
}
.end-info image{
    position: absolute;
    width: 336rpx;
    height: 272rpx;
    top: -150rpx;
    left: 50%;
    margin-left: -158rpx;
}
.icon-close{
    position: absolute;
    right: 24rpx;
    top: 20rpx;
    width: 48rpx;
    height: 48rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 36rpx 36rpx;
}
.share-in .icon-close, .share-content .icon-close{
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: 36rpx 36rpx;
}
.share-in{
    position: relative;
    width: 580rpx;
    background-image: linear-gradient(-45deg, #F64848 0%, #F095FF 100%);
    margin: 0 auto;
    margin-top: 200rpx;
    border-radius: 16rpx;
    color: #fff;
    padding-bottom: 40rpx;
}
.share-in .word-20{
  margin-top: 80rpx;
  padding:  0 40rpx;
  box-sizing: border-box;
}
.share-in .friend{margin-top: 30rpx;}
.share-in .invite-person{
    background: #fff;
    padding: 46rpx 24rpx 24rpx 24rpx;
    box-sizing: border-box;
    width: 92%;
    margin: 0 auto;
    margin-top: 40rpx;
    border-radius: 16rpx;
}
.share-in .invite-person .inline{
    width: 70rpx;
    height: 70rpx;
    margin-right: 0rpx;
    box-sizing: border-box;
}
.share-in .session-title{
    margin-top: 60rpx;
    padding: 0 40rpx;
}
.share-in .know-btn{
    width: 400rpx;
    height: 92rpx;
    line-height: 92rpx;
    background: none;
    font-size: 32rpx;
    border-radius: 46rpx;
    border: 2rpx solid #fff;
    margin-top: 40rpx;
    color: #fff;
}
.detail-content{
    padding: 20rpx;
    padding-bottom: 120rpx;
}
.share-in .user-name-h2{
    max-width: 280rpx;
    margin-top: 0rpx;
}